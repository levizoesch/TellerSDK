{
    "name": "levizoesch/teller-sdk",
    "description": "A simple SDK to consume Teller.io for Laravel.",
    "keywords": [
        "levizoesch",
        "teller",
        "teller.io",
        "api",
        "sdk",
        "open source",
        "laravel"
    ],
    "homepage": "https://github.com/levizoesch/teller-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Levi Zoesch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0|^8.0|^8.1|^8.2",
        "ext-curl": "*",
        "illuminate/contracts": "^7.0|^8.0|^9.0|^10.0",
        "levizoesch/laravelsetenvironment": "^1.0",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^3.0|^4.0|^5.0|^6.0|^7.0",
        "nunomaduro/collision": "^3.0|^4.0|^5.0|^6.0|^7.0",
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0",
        "pestphp/pest": "^2.24",
        "pestphp/pest-plugin-laravel": "^2.2",
        "phpunit/phpunit": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "TellerSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TellerSDK\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TellerSDK\\TellerSDKServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
