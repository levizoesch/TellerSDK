{
    "name": "levizoesch/teller-sdk",
    "description": "A simple SDK to consume Teller.io for Laravel.",
    "keywords": [
        "levizoesch",
        "teller",
        "teller.io",
        "api",
        "sdk",
        "open source",
        "laravel"
    ],
    "homepage": "https://github.com/levizoesch/teller-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Levi Zoesch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "ext-curl": "*",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.0|^9.0|^10.0",
        "levizoesch/laravelsetenvironment": "^1.0",
        "livewire/livewire": "^3.0",
        "rappasoft/laravel-livewire-tables": "v3.0.0-beta.4"
    },
    "require-dev": {
        "brianium/paratest": "^4.0|^5.0|^6.0|^7.0",
        "nunomaduro/collision": "^4.0|^5.0|^6.0|^7.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "LeviZoesch\\TellerSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LeviZoesch\\TellerSDK\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LeviZoesch\\TellerSDK\\TellerSDKServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
