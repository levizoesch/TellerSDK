{
    "name": "levizoesch/TellerSDK",
    "description": "A simple SDK to consume Teller.io for Laravel.",
    "keywords": [
        "levizoesch",
        "teller",
        "laravel"
    ],
    "homepage": "https://github.com/levizoesch/TellerSDK",
    "license": "MIT",
    "authors": [
        {
            "name": "Levi Zoesch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2"
    },
    "autoload": {
        "psr-4": {
            "LeviZoesch\\TellerSDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LeviZoesch\\TellerSDK\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
